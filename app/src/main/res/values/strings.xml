    <string name="password_guide1">Passwords must not include spaces or any punctuation marks like \'.\', \'!\', or \'?\'.</string>
    <string name="password_guide2">Avoid using easily guessable personal information such as your name, birthdate, or common words.</string>
    <string name="password_guide3">Do not use simple patterns like \'12345678\', \'22446688\', \'password\', or sequential keyboard patterns like \'qwerty\'.</string>
